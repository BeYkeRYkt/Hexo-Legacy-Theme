<?xml version="1.0" encoding="utf-8"?>
<familyset version="22">
    <!-- first font is default -->
    <family name="sans-serif">
        <font weight="100" style="normal">Lato-Thin.ttf</font>
        <font weight="100" style="italic">Lato-ThinItalic.ttf</font>
        <font weight="300" style="normal">Lato-Light.ttf</font>
        <font weight="300" style="italic">Lato-LightItalic.ttf</font>
        <font weight="400" style="normal">Lato-Regular.ttf</font>
        <font weight="400" style="italic">Lato-Italic.ttf</font>
        <font weight="500" style="normal">Lato-Medium.ttf</font>
        <font weight="500" style="italic">Lato-MediumItalic.ttf</font>
        <font weight="900" style="normal">Lato-Black.ttf</font>
        <font weight="900" style="italic">Lato-BlackItalic.ttf</font>
        <font weight="700" style="normal">Lato-Bold.ttf</font>
        <font weight="700" style="italic">Lato-BoldItalic.ttf</font>
    </family>

    <!-- Note that aliases must come after the fonts they reference. -->
    <alias name="sans-serif-thin" to="sans-serif" weight="100"/>
    <alias name="sans-serif-light" to="sans-serif" weight="300"/>
    <alias name="sans-serif-medium" to="sans-serif" weight="500"/>
    <alias name="sans-serif-black" to="sans-serif" weight="900"/>
    <alias name="arial" to="sans-serif"/>
    <alias name="helvetica" to="sans-serif"/>
    <alias name="tahoma" to="sans-serif"/>
    <alias name="verdana" to="sans-serif"/>

    <family name="sans-serif-condensed">
        <font weight="400" style="normal">LatoUI-BCondRegular.ttf</font>
        <font weight="400" style="italic">LatoUI-BCondItalic.ttf</font>
        <font weight="700" style="normal">LatoUI-BCondBold.ttf</font>
        <font weight="700" style="italic">LatoUI-BCondBoldItalic.ttf</font>
    </family>

    <!-- From Holo Theme -->
    <alias name="sans-serif-condensed-light" to="sans-serif-condensed" weight="300"/>

    <family name="serif">
        <font weight="400" style="normal">DroidSerif-Regular.ttf</font>
        <font weight="700" style="normal">DroidSerif-Bold.ttf</font>
        <font weight="400" style="italic">DroidSerif-Italic.ttf</font>
        <font weight="700" style="italic">DroidSerif-BoldItalic.ttf</font>
    </family>
    <alias name="times" to="serif"/>
    <alias name="times new roman" to="serif"/>
    <alias name="palatino" to="serif"/>
    <alias name="georgia" to="serif"/>
    <alias name="baskerville" to="serif"/>
    <alias name="goudy" to="serif"/>
    <alias name="fantasy" to="serif"/>
    <alias name="ITC Stone Serif" to="serif"/>

    <family name="monospace">
        <font weight="400" style="normal">DroidSansMono.ttf</font>
    </family>
    <alias name="sans-serif-monospace" to="monospace"/>
    <alias name="monaco" to="monospace"/>

    <family name="serif-monospace">
        <font weight="400" style="normal">DroidSansMono.ttf</font>
    </family>
    <alias name="courier" to="serif-monospace"/>
    <alias name="courier new" to="serif-monospace"/>

    <family name="casual">
        <font weight="400" style="normal">DroidSans.ttf</font>
    </family>

    <family name="cursive">
        <font weight="400" style="normal">DroidSerif-Regular.ttf</font>
        <font weight="700" style="normal">DroidSerif-Bold.ttf</font>
        <font weight="400" style="italic">DroidSerif-Italic.ttf</font>
        <font weight="700" style="italic">DroidSerif-BoldItalic.ttf</font>
    </family>

    <family name="sans-serif-smallcaps">
        <font weight="400" style="normal">Lato-Regular.ttf</font>
    </family>

    <!-- From old hexo -->
    <family>
        <nameset>
            <name>sans-serif</name>
            <name>arial</name>
            <name>helvetica</name>
            <name>tahoma</name>
            <name>verdana</name>
        </nameset>
        <fileset>
            <file>Lato-Regular.ttf</file>
            <file>Lato-Bold.ttf</file>
            <file>Lato-Italic.ttf</file>
            <file>Lato-BoldItalic.ttf</file>
        </fileset>
    </family>

    <family>
        <nameset>
            <name>sans-serif-light</name>
        </nameset>
        <fileset>
            <file>Lato-Light.ttf</file>
            <file>Lato-LightItalic.ttf</file>
        </fileset>
    </family>

    <family>
        <nameset>
            <name>sans-serif-thin</name>
        </nameset>
        <fileset>
            <file>Lato-Thin.ttf</file>
            <file>Lato-ThinItalic.ttf</file>
        </fileset>
    </family>

    <family>
        <nameset>
            <name>sans-serif-condensed</name>
        </nameset>
        <fileset>
            <file>LatoUI-BCondRegular.ttf</file>
            <file>LatoUI-BCondBold.ttf</file>
            <file>LatoUI-BCondItalic.ttf</file>
            <file>LatoUI-BCondBoldItalic.ttf</file>
        </fileset>
    </family>
</familyset>